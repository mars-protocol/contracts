{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "oneOf": [
    {
      "description": "Get config; returns `Config`",
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Get a single address; returns `AddressResponseItem`",
      "type": "object",
      "required": [
        "address"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/MarsContract"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Get a list of addresses; returns `Vec<AddressResponseItem>`",
      "type": "object",
      "required": [
        "addresses"
      ],
      "properties": {
        "addresses": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MarsContract"
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Query all stored contracts with pagination; returns `Vec<AddressResponseItem>`",
      "type": "object",
      "required": [
        "all_addresses"
      ],
      "properties": {
        "all_addresses": {
          "type": "object",
          "properties": {
            "limit": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint32",
              "minimum": 0.0
            },
            "start_after": {
              "anyOf": [
                {
                  "$ref": "#/definitions/MarsContract"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "MarsContract": {
      "type": "string",
      "enum": [
        "incentives",
        "oracle",
        "protocol_admin",
        "protocol_rewards_collector",
        "red_bank",
        "fee_collector",
        "safety_fund"
      ]
    }
  }
}
