{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "description": "Set addresses for contracts",
      "type": "object",
      "required": [
        "set_address"
      ],
      "properties": {
        "set_address": {
          "type": "object",
          "required": [
            "address",
            "contract"
          ],
          "properties": {
            "address": {
              "type": "string"
            },
            "contract": {
              "$ref": "#/definitions/MarsContract"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Propose to transfer the contract's ownership to another account",
      "type": "object",
      "required": [
        "transfer_ownership"
      ],
      "properties": {
        "transfer_ownership": {
          "type": "object",
          "required": [
            "new_owner"
          ],
          "properties": {
            "new_owner": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "MarsContract": {
      "type": "string",
      "enum": [
        "incentives",
        "oracle",
        "protocol_admin",
        "protocol_rewards_collector",
        "red_bank"
      ]
    }
  }
}
