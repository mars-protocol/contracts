{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "oneOf": [
    {
      "description": "Get config parameters; response: `Config<String>`",
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Get routes for swapping an input denom into an output denom; response: `RouteResponse`",
      "type": "object",
      "required": [
        "route"
      ],
      "properties": {
        "route": {
          "type": "object",
          "required": [
            "denom_in",
            "denom_out"
          ],
          "properties": {
            "denom_in": {
              "type": "string"
            },
            "denom_out": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Enumerate all swap routes; response: `RoutesResponse`",
      "type": "object",
      "required": [
        "routes"
      ],
      "properties": {
        "routes": {
          "type": "object",
          "properties": {
            "limit": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint32",
              "minimum": 0.0
            },
            "start_after": {
              "type": [
                "array",
                "null"
              ],
              "items": [
                {
                  "type": "string"
                },
                {
                  "type": "string"
                }
              ],
              "maxItems": 2,
              "minItems": 2
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
